.listClass {
	margin:5px 0px 8px 0px;
	width: 97%;
	position: absolute;
	left: 1%
}
.listInnerClass {
	margin:8px 7px 8px 2px; 
	border: 2px solid #CFEFBE;
	box-shadow:5px 5px 5px #777777;
	border-radius: 10px
}
._listInnerClass {
	margin:8px 7px 8px 2px; 
	border: 2px solid #FF2525;
	box-shadow:5px 5px 5px #777777;
	border-radius: 10px
}
.optionOutput{
	padding:0px 0px 0px 7px;
}
.contentOutput {
	padding :5px 3px 0px 5px;
}
.answerOutput{
	padding:0px 0px 5px 10px;
}
.showTitleClass{
	position:absolute;
	width:100%;
	height:100%;
	top:0px;
	background:rgba(243,243,243,0.7);
}
.showTitleInnerClass{
	position:absolute;
	width:60%;
	height:60%;
	top:20%;
	left:20%;
	border: 1px solid #cee3e9;
	background:white;
	box-shadow:5px 5px 5px #777777;
	border-radius: 10px;
}
.checkButton{
	position:relative;
	width:50%;
	padding:5px 5px 5px 5px;
	left:25%;
	margin:15px 2px 2px 2px;
}
.outputClass{
	font-family:"华文行楷";
	font-size:120%;
	margin: 10px 2px 1px 2px;
	padding:5px 7px 5px 7px;
}